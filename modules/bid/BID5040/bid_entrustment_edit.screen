            <a:dataSet id="bid5040_edit_entrustment_header_ds" autoQuery="true" fetchAll="true" model="bid.BID5040.bid_entrustment_headers" queryUrl="${/request/@context_path}/autocrud/bid.BID5040.bid_entrustment_headers/query?entrustment_header_id=${/parameter/@entrustment_header_id}">
                <a:fields>
                    <a:field name="entrustment_header_id"/>
