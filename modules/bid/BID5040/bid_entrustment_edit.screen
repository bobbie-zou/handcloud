    <a:view>
        <a:link id="bid5040_edit_entrustment_save_link" url="${/request/@context_path}/modules/bid/BID5040/bid_entrustment_save.svc"/>
        <a:link id="bid5040_edit_entrustment_submit_link" url="${/request/@context_path}/modules/bid/BID5040/bid_entrustment_submit.svc"/>
        <a:link id="bid5040_edit_entrustment_cancel_link" model="bid.BID5040.bid_cancel" modelaction="execute"/>
        <script><![CDATA[
            var bid5040_tax_type_id = null;
            var bid5040_currency_code_change_flag = false;
            var bid5040_currency_name_change_flag = false;
            
            function bid5040EntrustmentEditOnLoad(ds) {
                var record = ds.getCurrentRecord();
                var company_id = record.get('company_id');
                var using_company_id = record.get('using_company_id');
                var entrustment_type_id = record.get('entrustment_type_id');
                var functional_currency_code = record.get('functional_currency_code');
                var currency_code = record.get('currency_code');
                var suggested_bidding_method = record.get('suggested_bidding_method');
                bid5040_tax_type_id = record.get('tax_type_id');
            
                if (!Aurora.isEmpty(company_id)) {
                    record.getField('applicant_desc').setLovPara('company_id', company_id);
                }
                if (!Aurora.isEmpty(using_company_id)) {
                    record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                    record.getField('inv_organization_desc').setLovPara('company_id', using_company_id);
                }
                if (!Aurora.isEmpty(entrustment_type_id)) {
                    record.getField('tendering_tmpl_desc').setLovPara('entrustment_type_id', entrustment_type_id);
                }
                if (functional_currency_code != currency_code) {
                    record.getMeta().getField('exchange_rate').setReadOnly(false);
                    record.getMeta().getField('exchange_rate').setRequired(true);
                } else {
                    record.getMeta().getField('exchange_rate').setReadOnly(true);
                    record.getMeta().getField('exchange_rate').setRequired(false);
                }
                if (suggested_bidding_method == 'OPEN') {
                    $('bid5040_edit_entrustment_tab').setDisabled(1);
                } else if (suggested_bidding_method == 'INVITATION') {
                    $('bid5040_edit_entrustment_tab').setEnabled(1);
                }
            }
            
            function bid5040EntrustmentEditOnUpdate(ds, record, name, value, oldvalue) {
                if (name == "using_company_id") {
                    record.set('inv_organization_id', '');
                    record.set('inv_organization_desc', '');
            
                    if (!Aurora.isEmpty(value)) {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(false);
                        record.getField('inv_organization_desc').setLovPara('company_id', value);
                    } else {
                        record.getMeta().getField('inv_organization_desc').setReadOnly(true);
                    }
            
                    bid5040_currency_code_change_flag = true;
                    bid5040_currency_name_change_flag = true;
                    record.set('exchange_rate', 1);
                } else if (name == "entrustment_type_id") {
                    record.set('tendering_tmpl_id', '');
                    record.set('tendering_tmpl_desc', '');
                    record.getField('tendering_tmpl_desc').setLovPara('entrustment_type_id', value);
                } else if (name == "functional_currency_code") {
                    if (bid5040_currency_code_change_flag == true) {
                        record.set('currency_code', value);
            
                        bid5040_currency_code_change_flag = false;
                    }
                } else if (name == "functional_currency_name") {
                    if (bid5040_currency_name_change_flag == true) {
                        record.set('currency_name', value);
            
                        bid5040_currency_name_change_flag = false;
                    }
                } else if (name == "currency_code") {
                    var functional_currency_code = record.get('functional_currency_code');
                    if (functional_currency_code != value) {
                        record.getMeta().getField('exchange_rate').setReadOnly(false);
                        record.getMeta().getField('exchange_rate').setRequired(true);
                        record.set('exchange_rate', '');
                    } else {
                        record.getMeta().getField('exchange_rate').setReadOnly(true);
                        record.getMeta().getField('exchange_rate').setRequired(false);
                        record.set('exchange_rate', 1);
                    }
                } else if (name == 'suggested_bidding_method') {
                    if (value == 'OPEN') {
                        $('bid5040_edit_entrustment_tab').setDisabled(1);
                    } else if (value == 'INVITATION') {
                        $('bid5040_edit_entrustment_tab').setEnabled(1);
                    }
                }
            }
            
            function bid5040EntrustmentEditItemsOnAdd(ds, rec, index) {
                var header_ds = $('bid5040_edit_entrustment_header_ds');
                var header_rec = header_ds.getCurrentRecord();
            
                rec.set('need_by_date', header_rec.get('complete_date'));
                rec.set('tax_included_flag', header_rec.get('tax_included_flag'));
                rec.set('tax_type_id', header_rec.get('tax_type_id'));
                rec.set('tax_type_code', header_rec.get('tax_type_code'));
                rec.set('tax_type_rate', header_rec.get('tax_type_rate'));
            }
            
            function bid5040EntrustmentEditItemsOnUpdate(ds, record, name, value, oldvalue) {
                if (name == "base_price" || name == 'quantity' || name == 'tax_type_rate' || name == 'tax_included_flag') {
                    var base_price = record.get('base_price');
                    var quantity = record.get('quantity');
                    var unit_price = record.get('unit_price');
                    var net_price = record.get('net_price');
                    var net_amount = record.get('net_amount');
                    var tax_amount = record.get('tax_amount');
                    var total_amount = record.get('total_amount');
                    var tax_included_flag = record.get('tax_included_flag');
                    var tax_type_rate = record.get('tax_type_rate');
            
                    if (!Aurora.isEmpty(quantity) && !Aurora.isEmpty(base_price) && !Aurora.isEmpty(tax_type_rate)) {
                        if (tax_included_flag == 'N') {
                            record.set('unit_price', parseFloat((base_price * (1 + tax_type_rate / 100)).toFixed(8)));
                            record.set('net_price', parseFloat(base_price.toFixed(8)));
                            unit_price = record.get('unit_price');
                            net_price = record.get('net_price');
                            record.set('net_amount', parseFloat((net_price * quantity).toFixed(2)));
                            record.set('tax_amount', parseFloat(((unit_price - net_price) * quantity).toFixed(2)));
                            net_amount = record.get('net_amount');
                            tax_amount = record.get('tax_amount');
                            record.set('total_amount', parseFloat((net_amount + tax_amount).toFixed(2)));
                        } else if (tax_included_flag == 'Y') {
                            record.set('unit_price', parseFloat(base_price.toFixed(8)));
                            record.set('net_price', parseFloat((base_price / (1 + (tax_type_rate / 100))).toFixed(8)));
                            unit_price = record.get('unit_price');
                            net_price = record.get('net_price');
                            record.set('net_amount', parseFloat((net_price * quantity).toFixed(2)));
                            record.set('tax_amount', parseFloat(((unit_price - net_price) * quantity).toFixed(2)));
                            net_amount = record.get('net_amount');
                            tax_amount = record.get('tax_amount');
                            record.set('total_amount', parseFloat((net_amount + tax_amount).toFixed(2)));
                        }
                    } else {
                        record.set('unit_price', '');
                        record.set('net_price', '');
                        record.set('net_amount', '');
                        record.set('tax_amount', '');
                        record.set('total_amount', '');
                    }
                }
            }
            
            function bid5040EntrustmentEditSave() {
                var header_ds = $('bid5040_edit_entrustment_header_ds');
                var item_ds = $('bid5040_edit_entrustment_items_ds');
                var bidder_ds = $('bid5040_edit_entrustment_bidders_ds');
            
                if (!header_ds.validate()) {
                    return;
                }
                if (!item_ds.validate()) {
                    return;
                }
                if (!bidder_ds.validate()) {
                    return;
                }
            
                var data = header_ds.getCurrentRecord().data;
                data['items'] = item_ds.getJsonData();
                data['bidders'] = bidder_ds.getJsonData();
            
                Aurora.Masker.mask($('bid5040_entrustment_edit_window').wrap, '${l:LOADING}');
            
                Aurora.request({
                    url: $('bid5040_edit_entrustment_save_link').getUrl(),
                    para: data,
                    success: function() {
                        Aurora.showMessage('${l:PROMPT}', '${l:BID_ENTRUSTMENT_HEADERS.SAVED_MSG}!', function() {
                            header_ds.query();
                            item_ds.query();
                            bidder_ds.query();
                        });
                    },
                    failure: function() {
                        return;
                    },
                    error: function() {
                        return;
                    }
                });
            
                Aurora.Masker.unmask($('bid5040_entrustment_edit_window').wrap);
            }
            
            function bid5040EntrustmentEditSubmit() {
                var header_ds = $('bid5040_edit_entrustment_header_ds');
                var item_ds = $('bid5040_edit_entrustment_items_ds');
                var bidder_ds = $('bid5040_edit_entrustment_bidders_ds');
            
                if (!header_ds.validate()) {
                    return;
                }
                if (!item_ds.validate()) {
                    return;
                }
                if (!bidder_ds.validate()) {
                    return;
                }
            
                var data = header_ds.getCurrentRecord().data;
                data['items'] = item_ds.getJsonData();
                data['bidders'] = bidder_ds.getJsonData();
                Aurora.showConfirm('${l:PROMPT}', '${l:BID_ENTRUSTMENT_HEADERS.CONFIRM_SUBMIT_MSG}？', function() {
                    Aurora.Masker.mask($('bid5040_entrustment_edit_window').wrap, '${l:LOADING}');
            
                    Aurora.request({
                        url: $('bid5040_edit_entrustment_submit_link').getUrl(),
                        para: data,
                        success: function() {
                            Aurora.showMessage('${l:PROMPT}', '${l:BID_ENTRUSTMENT_HEADERS.SUBMITED_MSG}!', function() {
                                $('bid5040_entrustment_result_ds').query();
                                $('bid5040_entrustment_edit_window').close();
                            });
                        },
                        failure: function() {
                            return;
                        },
                        error: function() {
                            return;
                        }
                    });
            
                    Aurora.Masker.unmask($('bid5040_entrustment_edit_window').wrap);
                });
            }
            
            function bid5040EntrustmentEditCancel() {
                var header_ds = $('bid5040_edit_entrustment_header_ds');
                var data = header_ds.getCurrentRecord().data;
            
                Aurora.showConfirm('${l:PROMPT}', '${l:BID_ENTRUSTMENT_HEADERS.CONFIRM_CANCEL_MSG}？', function() {
                    Aurora.Masker.mask($('bid5040_entrustment_edit_window').wrap, '${l:LOADING}');
            
                    Aurora.request({
                        url: $('bid5040_edit_entrustment_cancel_link').getUrl(),
                        para: data,
                        success: function() {
                            Aurora.showMessage('${l:PROMPT}', '${l:BID_ENTRUSTMENT_HEADERS.CANCELED_MSG}!', function() {
                                $('bid5040_entrustment_result_ds').query();
                                $('bid5040_entrustment_edit_window').close();
                            });
                        },
                        failure: function() {
                            return;
                        },
                        error: function() {
                            return;
                        }
                    });
            
                    Aurora.Masker.unmask($('bid5040_entrustment_edit_window').wrap);
                });
            }
            
            function bid5040EntrustmentEditReturn() {
                $('bid5040_entrustment_result_ds').query();
                $('bid5040_entrustment_edit_window').close();
            }
            
            function bid5040EntrustmentItemEditEditor(rec, name) {
                if (name == 'tax_type_code') {
                    if (!Aurora.isEmpty(bid5040_tax_type_id)) {
                        return '';
                    } else {
                        return 'bid5040_edit_entrustment_items_lov';
                    }
                }
            }
            
            function bid5040EntrustmentItemEditRender(value, rec, name) {
                if (name == 'quantity') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 2);
                    }
                    return value;
                } else if (name == 'base_price') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 8);
                    }
                    return value;
                } else if (name == 'unit_price') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 8);
                    }
                    return value;
                } else if (name == 'net_price') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 8);
                    }
                    return value;
                } else if (name == 'net_amount') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 2);
                    }
                    return value;
                } else if (name == 'tax_amount') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 2);
                    }
                    return value;
                } else if (name == 'total_amount') {
                    if (!Aurora.isEmpty(value)) {
                        return Aurora.formatNumber(value, 2);
                    }
                    return value;
                }
            }
            
            function bid5040EntrustmentEditAtmUpload() {
                if ('${/parameter/@entrustment_header_id}') {
                    var url = "${/request/@context_path}/uploadFile.screen?table_name=BID_ENTRUSTMENT_HEADERS&header_id=${/parameter/@entrustment_header_id}";
                    new Aurora.Window({
                        url: url,
                        title: '${l:ATM.UPLOAD_ATTACHMENT}',
                        id: 'bid5040_entrustment_edit_atm_window',
                        width: 350,
                        height: 400
                    }).on('beforeclose', function() {
                        $('bid5040_edit_atm_files_ds').query();
                    });
                }
            }
            
            function bid5040EntrustmentEditAtmRender(value, rec, name) {
                if (name == 'file_download_link') {
                    var file_download_link = rec.get('file_download_link');
                    return file_download_link;
                }
            }
        ]]></script>
