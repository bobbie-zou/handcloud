        <script><![CDATA[
        	function pur7110_init(){
        	    var query_ds = $('pur7110_rfx_header_query_ds');
        	    var query_rec = query_ds.getCurrentRecord();
        	    query_rec.getField('created_by_desc').setLovPara('user_type','INTERNAL');
        	}
        	
        	Aurora.onReady(function(){
        	    pur7110_init();
        	});
        ]]></script>
