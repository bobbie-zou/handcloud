            <a:dataSet id="pur7110_rfx_auction_ranking_ds" lookupCode="PUR_RFX_AUCTION_RANKING"/>
            <a:dataSet id="pur7110_rfx_status_ds" lookupCode="PUR_RFX_STATUS"/>
