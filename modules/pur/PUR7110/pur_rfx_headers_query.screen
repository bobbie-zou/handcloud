<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure>
        <a:model-query fetchAll="true" model="public.pur_rfx_types_list" rootPath="pur7110_rfx_types_source"/>
        <a:model-query fetchAll="true" model="public.csh_payment_methods_list" rootPath="pur7110_payment_methods_source"/>
        <a:model-query fetchAll="true" model="public.currency_list" rootPath="pur7110_currency_source"/>
        <a:model-query fetchAll="true" model="public.fnd_tax_type_code_lov" rootPath="pur7110_tax_type_code_source"/>
    </a:init-procedure>
    <a:view>
        <a:link id="pur7110_rfx_details_link" url="${/request/@context_path}/modules/pur/PUR7110/pur_rfx_details.screen"/>
        <script><![CDATA[

		function pur7110_query(){
        	 $('pur7110_rfx_header_result_ds').query();
        }  

		function pur7110_rfx_render(value,record,name){
        	    var rfx_header_id=record.get('rfx_header_id');
        	    return '<a href="javascript:pur7110_rfx_window('+rfx_header_id+')">'+value+'</a>';
        }    
        	
        function pur7110_rfx_window(id){
            var window_id;
            var url;
            window_id = 'pur71101_rfx_details_window'; 
            url = $('pur7110_rfx_details_link').getUrl();  
            url += '?rfx_header_id='+id;
        	new Aurora.Window({
        	    id:window_id,
        	    title:'${l:MODULES/PUR/PUR7110/PUR_RFX_DETAILS.SCREEN.TITLE}',
        	    url:url,
        	    fullScreen:true
        	}).on('beforeclose', pur7110_query);
        }   



		]]></script>
        <a:dataSets>
            <a:dataSet id="pur7110_rfx_category_ds" lookupCode="PUR_RFX_CATEGORY"/>
            <a:dataSet id="pur7110_rfx_method_ds" lookupCode="PUR_RFX_METHOD"/>
            <a:dataSet id="pur7110_rfx_auction_direction_ds" lookupCode="PUR_RFX_AUCTION_DIRECTION"/>
