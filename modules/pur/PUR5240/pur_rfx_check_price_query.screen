                        </a:events>
                    </a:textField>
                    <a:textField name="title" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.TITLE">
                        <a:events>
                            <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                        </a:events>
                    </a:textField>
                </a:formToolBar>
                <a:formBody>
                    <a:hBox>
                        <a:lov name="company_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.COMPANY_ID">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:lov>
                        <a:comboBox name="rfx_type_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.RFX_TYPE_ID">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                        <a:comboBox name="rfx_category_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.RFX_CATEGORY">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                        <a:comboBox name="rfx_method_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.RFX_METHOD">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                    </a:hBox>
                    <a:hBox>
