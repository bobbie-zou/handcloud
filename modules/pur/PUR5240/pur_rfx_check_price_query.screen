                    <a:column name="created_by_desc" align="left" prompt="PUR_RFX_HEADERS.CREATED_BY" width="60"/>
                    <a:column name="creation_date_desc" align="left" prompt="PUR_RFX_HEADERS.CREATION_DATE" width="60"/>
