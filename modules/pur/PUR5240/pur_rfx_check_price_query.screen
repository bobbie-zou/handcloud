                <a:columns>
                    <a:column name="feedback_header_id" align="center" lock="true" prompt="MODULES/PUR/PUR5240/PUR_RFX_CHECK_PRICE.SCREEN.TITLE" renderer="pur5240_rfx_check_price_details_render" sortable="true" width="50"/>
                    <a:column name="rfx_number" align="left" lock="true" prompt="PUR_RFX_HEADERS.RFX_NUMBER" width="80"/>
                    <a:column name="title" align="left" lock="true" prompt="PUR_RFX_HEADERS.TITLE" width="100"/>
                    <a:column name="company_desc" align="left" prompt="PUR_RFX_HEADERS.COMPANY_ID" width="80"/>
                    <a:column name="rfx_type_desc" align="left" prompt="PUR_RFX_HEADERS.RFX_TYPE_ID" width="80"/>
                    <a:column name="rfx_category_desc" align="left" prompt="PUR_RFX_HEADERS.RFX_CATEGORY" width="60"/>
                    <a:column name="rfx_method_desc" align="left" prompt="PUR_RFX_HEADERS.RFX_METHOD" width="40"/>
                    <a:column name="auction_direction_desc" align="left" prompt="PUR_RFX_HEADERS.AUCTION_DIRECTION" width="40"/>
                    <a:column name="auction_rule_desc" align="left" prompt="PUR_RFX_HEADERS.AUCTION_RULE" width="80"/>
                    <a:column name="open_rule_desc" align="left" prompt="PUR_RFX_HEADERS.OPEN_RULE" width="80"/>
                    <a:column name="ranking_rule_desc" align="left" prompt="PUR_RFX_HEADERS.RANKING_RULE" width="80"/>
                    <a:column name="auction_ranking_desc" align="left" prompt="PUR_RFX_HEADERS.AUCTION_RANKING" width="80"/>
