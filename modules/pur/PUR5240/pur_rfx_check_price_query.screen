                            </a:events>
                        </a:comboBox>
                        <a:comboBox name="ranking_rule_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.RANKING_RULE">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                    </a:hBox>
                    <a:hBox>
                        <a:comboBox name="auction_ranking_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.AUCTION_RANKING">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                        <a:comboBox name="status_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.STATUS">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                        <a:comboBox name="currency_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.CURRENCY_CODE">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:comboBox>
                        <a:lov name="created_by_desc" bindTarget="pur5240_rfx_feedback_query_ds" prompt="PUR_RFX_HEADERS.CREATED_BY">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:lov>
                    </a:hBox>
                    <a:hBox>
                        <a:datePicker name="creation_date_from" bindTarget="pur5240_rfx_feedback_query_ds" prompt="SACPUR5210.CREATION_DATE_FROM">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:datePicker>
                        <a:datePicker name="creation_date_to" bindTarget="pur5240_rfx_feedback_query_ds" prompt="SACPUR5210.CREATION_DATE_TO">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:datePicker>
                        <a:datePicker name="issued_date_from" bindTarget="pur5240_rfx_feedback_query_ds" prompt="SACPUR5210.ISSUED_DATE_FROM">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:datePicker>
                        <a:datePicker name="issued_date_to" bindTarget="pur5240_rfx_feedback_query_ds" prompt="SACPUR5210.ISSUED_DATE_TO">
                            <a:events>
                                <a:event name="enterdown" handler="pur5240_rfx_check_price_result_query"/>
                            </a:events>
                        </a:datePicker>
                    </a:hBox>
                    <a:hBox>
                        <a:datePicker name="feedback_start_time_from" bindTarget="pur5240_rfx_feedback_query_ds" prompt="SACPUR5210.FEEDBACK_START_TIME_FROM">
